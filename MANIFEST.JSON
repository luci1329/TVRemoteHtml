{
  "manifest_version": 2,
  "name": "TV Remote Extension",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.jpg"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
